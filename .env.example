DATABASE_URL=mysql://root:1234567890@localhost:3306/arxiv_db
ARXIV_CATEGORIES=cs.AI,cs.LG,cs.CV,cs.CL,cs.NE
RATE_LIMIT_SECONDS=4
MAX_RESULTS_PER_SEARCH=10

   # Embedding Configuration
EMBEDDING_PROVIDER=sentence-transformers  # or "openai"
EMBEDDING_MODEL=sentence-transformers/all-MiniLM-L6-v2
OPENAI_API_KEY=your_key_here  # If using OpenAI
OPENAI_EMBEDDING_MODEL=text-embedding-3-small


# Qdrant Configuration
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION_NAME=paper_chunks
# OR for cloud Qdrant:
# QDRANT_URL=https://your-cluster.qdrant.io
# QDRANT_API_KEY=your_api_key

# Chunking Configuration (optional)
CHUNK_SIZE_TOKENS=650
CHUNK_OVERLAP_TOKENS=125